{
  "name": "remote-media",
  "version": "1.0.0-beta.4",
  "description": "A small app to control other client's media remotely.",
  "main": "server.js",
  "scripts": {
    "start": "node build/src/index.js",
    "start:dev": "nodemon --config nodemon.json",
    "compile": "tsc",
    "lint": "gts lint",
    "clean": "gts clean",
    "fix": "gts fix",
    "prepare": "npm.cmd run compile",
    "pretest": "npm.cmd run compile",
    "posttest": "npm.cmd run lint",
    "test": "nyc mocha",
    "testWithReport": "nyc --reporter=cobertura --reporter=text mocha test --reporter mocha-junit-reporter"
  },
  "author": "banksio",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "app-root-path": "^3.0.0",
    "bootstrap": "^4.5.3",
    "chalk": "^4.1.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "socket.io": "^4.0.1",
    "socket.io-client": "^4.0.1",
    "youtube-playlist": "^1.0.4"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/express": "^4.17.11",
    "eslint": "^7.15.0",
    "gts": "^3.1.0",
    "mocha": "^8.2.1",
    "mocha-junit-reporter": "^2.0.0",
    "mock-socket": "^9.0.3",
    "nyc": "^15.1.0",
    "sinon": "^9.2.1",
    "typescript": "^4.2.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/banksio/remote-media.git"
  },
  "bugs": {
    "url": "https://github.com/banksio/remote-media/issues"
  },
  "homepage": "https://github.com/banksio/remote-media#readme",
  "jshintConfig": {
    "esversion": 10
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "sync",
    "media",
    "YouTube",
    "remote"
  ]
}
