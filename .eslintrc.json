{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 10
    },
    "rules": {
        "no-unused-vars":"warn",
        "no-empty":"warn",
        "no-useless-escape":"off",
        "no-setter-return": "off",
        "no-undef":"off"
    }
}
